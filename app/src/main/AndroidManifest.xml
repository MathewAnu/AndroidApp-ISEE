<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.manishrchandran.AndroidApp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        android:launch=
        <activity android:name="com.example.manishrchandran.AndroidApp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.manishrchandran.AndroidApp.Security" />
        <activity android:name="com.example.manishrchandran.AndroidApp.MainMenu" />
        <activity android:name="com.example.manishrchandran.AndroidApp.EnterActivity" />
        <activity android:name="com.example.manishrchandran.AndroidApp.ViewAllEntries" />
        <activity android:name="com.example.manishrchandran.AndroidApp.SecurityQuestion" />
        <activity android:name="com.example.manishrchandran.AndroidApp.ExportData" />
        <activity android:name="com.example.manishrchandran.AndroidApp.AnalyseActivity"/>
        <activity android:name="com.example.manishrchandran.AndroidApp.SendFeedback"/>

    </application>
    <uses-permission android:name="android.permission.INTERNET"/>

</manifest>